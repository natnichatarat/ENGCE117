#include <stdio.h>
	int	 main()
	{
		int num,sum=9,n=9,m=9;
		printf("User Input :");
		scanf ("%d",&num);
		printf ("---------\n");
		printf("Series =");
		printf("9");
		for(int i = 1 ; i < num ; i++){ 
		  n=n*10;
		  m= m+n;
		  sum = m+sum;
          printf("+%d",m);
     } //end loop for
        printf("\n");
       printf("Sum = %d",sum);
	 return 0;		
	}
